{
    "presets": [
        [
            "@babel/preset-env",
            {
                "loose": true,
                "modules": false
            }
        ],
        "@babel/react"
    ],
    "plugins": [
        [
          "formatjs",
          {
            "idInterpolationPattern": "[sha512:contenthash:base64:6]",
            "ast": true
          }
        ],
        [
          "module-resolver",
          {
            "root": ["./src"],
            "alias": {
              "": "./src"
            }
          }
        ]
    ]
}
